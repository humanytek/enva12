<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<template id="report_deliveryship_nova_inherit" inherit_id="stock.report_delivery_document">
  <xpath expr="//table[@name='stock_move_line_table']/thead/tr/th[@name='th_sml_product']" position="after">
    <th name="th_sml_account_analytic"><strong>Cuenta Analitica</strong></th>
  </xpath>

</template>

<template id="stock_report_delivery_aggregated_move_lines_nova_inherit" inherit_id="stock.stock_report_delivery_aggregated_move_lines">
  <xpath expr="//tr/td">
    <td>
        <span t-esc="aggregated_lines[line]['account_analytic']"/>
    </td>
  </xpath>
</template>
</odoo>
