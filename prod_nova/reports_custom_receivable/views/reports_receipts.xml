          <field name="tag">account_report</field>
          <field name="context" eval="{'model': 'reports.receipts.nova'}" />
      </record>
      <menuitem id="menu_action_reports_receipts_nova" name="Reporte de Ingresos Corrugado" action="action_reports_receipts_nova" parent="reports_custom_receivable.menu_reports_receivable_nova_account" groups="account.group_account_user" sequence="60"/>

      <record id="action_reports_receipts_nova_paper" model="ir.actions.client">
          <field name="name">Reporte de ingresos Papel</field>
          <field name="tag">account_report</field>
          <field name="context" eval="{'model': 'reports.receipts.nova.paper'}" />
      </record>
      <menuitem id="menu_action_reports_receipts_nova_paper" name="Reporte de Ingresos Papel" action="action_reports_receipts_nova_paper" parent="reports_custom_receivable.menu_reports_receivable_nova_account" groups="account.group_account_user" sequence="61"/>

      <record id="action_reports_receipts_nova_acum" model="ir.actions.client">
